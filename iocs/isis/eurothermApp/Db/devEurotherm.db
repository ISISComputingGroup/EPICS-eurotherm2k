record(bo, "$(P):SIM")
{
    field(SCAN, "Passive")
    field(DTYP, "Soft Channel")
    field(ZNAM, "NO")
    field(ONAM, "YES")
}

record(bo, "$(P):DISABLE") 
{
    field(DESC, "Disable comms")
    field(PINI, "YES")
    field(VAL, "0")
    field(OMSL, "supervisory")
    field(ZNAM, "COMMS ENABLED")
    field(ONAM, "COMMS DISABLED")
}

record(ai, "$(P):TEMPERATURE") {
    field(SCAN, "1 second")
    field(DESC, "Temperature Readback")
    field(DTYP, "stream")
    field(INP, "@eurotherm2k.proto read($(GAD),$(LAD),PV) $(PORT)")
    field(SIML, "$(P):SIM")
    field(SIOL, "$(P):SIM:TEMPERATURE")
    field(SDIS, "$(P):DISABLE")
}

record(ai, "$(P):TEMPERATURE:SP:RBV") {
    field(SCAN, "1 second")
    field(DESC, "Temperature Setpoint Readback")
    field(DTYP, "stream")
    field(INP, "@eurotherm2k.proto read($(GAD),$(LAD),SP) $(PORT)")
    field(SIML, "$(P):SIM")
    field(SIOL, "$(P):SIM:TEMPERATURE:SP:RBV")
    field(SDIS, "$(P):DISABLE")
}

record(ao, "$(P):TEMPERATURE:SP") {
    field(DESC, "Temperature Setpoint")
    field(DTYP, "stream")
